              <stringProp name="cacheKey">false</stringProp>
              <stringProp name="script">def response = prev.getResponseDataAsString()
def regex = /VIEWSTATE&quot; value=&quot;(.*?)&quot;/  
def viewStateRegEx = ( response =~ regex)
def viewStateGeneratorRegEx = ( response =~ &quot;VIEWSTATEGENERATOR.*?value=.(.*?)\&quot;&quot;)
def eventValidatorRegEx = ( response =~ &quot;EVENTVALIDATION.*?value=.(.*?)\&quot;&quot;)
//
vars.put(&apos;view_state&apos;, viewStateRegEx[0][1].toString())
vars.put(&apos;view_state_generator&apos;,  viewStateGeneratorRegEx[0][1].toString())
vars.put(&apos;event_validator&apos;, eventValidatorRegEx[0][1].toString())

log.warn(vars.get(&apos;view_state_generator&apos;))
</stringProp>
            </JSR223PostProcessor>
